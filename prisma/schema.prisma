generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Todo {
  id         Int      @id @default(autoincrement())
  title      String
  desc       String
  category   String
  prioritas  String
  deadline   DateTime
  createdAt  DateTime @default(now())
}
